                {{ msg }}
            </div>
        {% endfor %}
        <!-- SUCCESS MESSAGE end-->

        {% block body %}{% endblock %}

        <!-- Footer section -->
      	<footer class="footer-section">
      		<div class="container footer-top">
      			<div class="row">
      				<!-- widget -->
      				<div class="col-sm-6 col-lg-3 footer-widget">
      					<div class="about-widget">
      						<img src="{{ asset('img/logo-ianca.jpg') }}" alt="">
      					</div>
      				</div>
      				<!-- widget -->
      				<div class="col-sm-6 col-lg-3 footer-widget">
      					<h6 class="fw-title">DE INTERES</h6>
      					<div class="dobule-link">
      						<ul>
      							<li><a href="{{ path('poder-adquisitivo') }}">Poder adquisitivo</a></li>
      							<li><a href="{{ path('registros-profesionales') }}">Registros profesionales</a></li>
      							<!--li><a href="#">Actividades</a></li-->
      							<li><a href="{{ path('faq') }}">Preguntas frecuentes</a></li>
      							<li><a href="{{ path('que-es') }}">¿Qué es …?</a></li>
      							<!--li><a href="#">Sitios de interés</a></li-->
                                <li><a href="{{ path('contacto') }}">Contáctenos</a></li>
      						</ul>
      					</div>
      				</div>
      				<div class="col-sm-6 col-lg-3 footer-widget">
      					<h6 class="fw-title">CONTACTO</h6>
      					<ul class="contact">
      						<li><p><i class="fa fa-map-marker"></i> Fragata Sarmiento 46, Buenos Aires</p></li>
      						<li><p><i class="fa fa-phone"></i> (005411) 2054-1499</p></li>
      						<li><p><i class="fa fa-mobile"></i> 6668-5935 | 4094-5536</p></li>
                  <li><p><i class="fa fa-envelope"></i> info@ianca.com.ar</p></li>
      					</ul>
      				</div>
      			</div>
      		</div>
      		<!-- copyright -->
      		<div class="copyright">
      			<div class="container">
      				<p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
      			</div>
      		</div>
      	</footer>
      	<!-- Footer section end-->

      	<!--====== Javascripts & Jquery ======-->
      	<script src="{{ asset('js/jquery-3.2.1.min.js') }}"></script>
      	<script src="{{ asset('js/owl.carousel.min.js') }}"></script>
      	<script src="{{ asset('js/jquery.countdown.js') }}"></script>
      	<script src="{{ asset('js/masonry.pkgd.min.js') }}"></script>
      	<script src="{{ asset('js/magnific-popup.min.js') }}"></script>
      	<script src="{{ asset('js/main.js') }}"></script>
        <script type="text/javascript">
            setTimeout(function(){
                if ($('#divSuccess').length > 0) {
                    $('#divSuccess').toggle("slow");
                }
            }, 5000)
        </script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
